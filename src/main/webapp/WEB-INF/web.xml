<?xml version="1.0" encoding="UTF-8"?>
<web-app  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
  <display-name>gestionReservation</display-name>
  <welcome-file-list>

    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
        <servlet-name>UtilisateurServlet</servlet-name>
        <servlet-class>gestionReservation.servlet.UtilisateurServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UtilisateurServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
      <servlet>
        <servlet-name>DashBoardServlet</servlet-name>
        <servlet-class>gestionReservation.servlet.DashBoardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DashBoardServlet</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
       <servlet>
        <servlet-name>GestionCategoryServlet</servlet-name>
        <servlet-class>gestionReservation.servlet.GestionCategoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GestionCategoryServlet</servlet-name>
        <url-pattern>/gestionCategory</url-pattern>
    </servlet-mapping>
        <servlet>
        <servlet-name>GestionClientServlet</servlet-name>
        <servlet-class>gestionReservation.servlet.GestionClient</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GestionClientServlet</servlet-name>
        <url-pattern>/gestionClient</url-pattern>
    </servlet-mapping>
   <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>
</web-app>